<!--pages/category/childCpns/w-menu/w-menu.wxml-->
<scroll-view class='menu' scroll-y>
  <block wx:for="{{categories}}" wx:key="index">
    <view class='item {{index == currentIndex ? "active": ""}}' 
          bindtap='onItemClick' 
          data-index="{{index}}">
      {{item.title}}
    </view>
  </block>
</scroll-view>
